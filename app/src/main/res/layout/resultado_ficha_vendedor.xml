<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ActivityResultadosEjercicio1">

    <TextView
        android:id="@+id/tvnombre"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:padding="10dp"
        android:text="Nombre"
        android:textSize="26sp"
        app:layout_constraintBottom_toTopOf="@+id/tvcodigo"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tvcodigo"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:padding="10dp"
        android:text="Codigo"
        android:textSize="26sp"
        app:layout_constraintBottom_toTopOf="@+id/tvmes"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvnombre" />

    <TextView
        android:id="@+id/tvComision"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="10dp"
        android:text="Comision:"
        android:textSize="26sp"
        app:layout_constraintBottom_toTopOf="@+id/tvVentas"
        app:layout_constraintTop_toBottomOf="@+id/tvmes" />

    <TextView
        android:id="@+id/tvVentas"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:padding="10dp"
        android:text="Ventas:"
        android:textSize="26sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvComision" />

    <TextView
        android:id="@+id/tvmes"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="10dp"
        android:text="Mes:"
        android:textSize="26sp"
        app:layout_constraintBottom_toTopOf="@+id/tvComision"
        app:layout_constraintTop_toBottomOf="@+id/tvcodigo" />

</androidx.constraintlayout.widget.ConstraintLayout>